import json
import sqlite3

class Strawberries_Registry:

	print('here00')

	def __init__(self):
		self._db_path = 'strawberries.db'

	def insert_strawberries_data(self):
		print('here0')
		with open('strawberries.json') as f:
			strawberries_data = json.loads(f.read())

			#List of the columns in the JSON file.
			db_fields = []
			columns_in_file = []
			for data in strawberries_data:
				columns_in_file = list(data.keys())
				for col in columns_in_file:
					if col not in db_fields:
						db_fields.append(col)
			print('here1')
			print(db_fields)

			#Values of the columns in the JSON file in the right order. 
			db_insert_values = []
			values_in_file = [] 
			for data in strawberries_data:
				for i in db_fields:
					db_insert_values = append(str(dict(data).get(i)))   
				values_in_file.append(list(db_insert_values)) 
				db_insert_values.clear()
			print('here2')
			print(db_insert_values)




		with sqlite3.connect(self._dp_path) as connection:
			cursor = connection.cursor()



s = Strawberries_Registry()
print(s.insert_strawberries_data())
print('hello')



